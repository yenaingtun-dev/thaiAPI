<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Stock Data</title>
    <!-- Bootstrap CSS -->
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css"
      rel="stylesheet"
    />
    <!-- DataTables CSS -->
    <link
      rel="stylesheet"
      href="https://cdn.datatables.net/1.11.5/css/dataTables.bootstrap5.min.css"
    />
  </head>

  <body>
    <div class="container mt-5">
      <h2>Stock Data</h2>
      <table id="stockTable" class="table table-bordered table-striped">
        <thead>
          <tr>
            <th>SET</th>
            <th>Value</th>
            <th>Open Time</th>
            <th>2D</th>
            <th>Stock Date</th>
            <th>Stock Datetime</th>
            <th>History ID</th>
          </tr>
        </thead>
        <tbody>
          <% stockDatas.forEach(function(stockData) { %>
          <tr>
            <td><%= stockData.set %></td>
            <td><%= stockData.value %></td>
            <td><%= stockData.open_time %></td>
            <td><%= stockData.twod %></td>
            <td><%= stockData.stock_date %></td>
            <td><%= stockData.stock_datetime %></td>
            <td><%= stockData.history_id %></td>
          </tr>
          <% }); %>
        </tbody>
      </table>
    </div>
    <!-- Bootstrap JS -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"></script>
    <!-- jQuery -->
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <!-- DataTables JS -->
    <script src="https://cdn.datatables.net/1.11.5/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.11.5/js/dataTables.bootstrap5.min.js"></script>
    <!-- Initialize DataTable -->
    <script>
      $(document).ready(function () {
        $("#stockTable").DataTable({
          paging: true,
          searching: true,
          ordering: true,
          "order": [[5, 'desc']]
        });
      });
    </script>
  </body>
</html>
